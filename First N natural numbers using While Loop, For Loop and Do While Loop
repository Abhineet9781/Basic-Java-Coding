// Using While Loop 



import java.util.Scanner;
public class LearnCoding {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        System.out.print("Enter a number: ");
        int num = scanner.nextInt();

        int i = 1;

        while(i<=num){
            System.out.print(i + " ");
            i++;
        }
    }
}





// Using For Loop

public class LearnCoding {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        System.out.print("Enter a number: ");
        int num = scanner.nextInt();

        for (int i=1;i<=num;i++){
            System.out.print(i+" ");
        }
    }
}




// Using Do While Loop

import java.util.Scanner;
public class LearnCoding {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        System.out.print("Enter a number: ");
        int num = scanner.nextInt();

        int i = 1;

        do{
            System.out.print(i + " ");
            i++;
        }while (i<=num);
    }
}
